								"exec": [
									"// Basic response sanity tests",
									"pm.test(\"Status code is 2xx\", function () {",
									"  pm.expect(pm.response.code).to.be.within(200, 299);",
									"});",
									"",
									"pm.test(\"Response has JSON content-type when body exists\", function () {",
									"  if (pm.response.text()) {",
									"    pm.response.to.have.header(\"Content-Type\");",
									"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");",
									"  }",
									"});",
									"",
									"// Validate expected order shape",
									"let asJson;",
									"try {",
									"  asJson = pm.response.json();",
									"} catch (e) {",
									"  asJson = null;",
									"}",
									"",
									"if (asJson) {",
									"  pm.test(\"Response contains order id\", function () {",
									"    // Accept either { id: ... } or { data: { id: ... } } shapes",
									"    const id = (asJson && asJson.id) || (asJson && asJson.data && asJson.data.id) || (asJson && asJson.order && asJson.order.id);",
									"    pm.expect(id, \"order id present\").to.exist;",
									"  });",
